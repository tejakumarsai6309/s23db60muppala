extends layout

block content
  h1= title
  - const image_names = ['ferrari.jpg', 'mustang yellow.jpg', 'mustang.jpg', 'tesla black.jpg', 'tesla white.jpg']

  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    
    img(
      src=`/images/${image_names[randomIndex]}`,
      alt=`Image ${randomIndex + 1}`,
      height=300,
      width=300
    )